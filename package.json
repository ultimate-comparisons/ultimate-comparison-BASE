{
  "name": "ultimate-comparison-framework",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run gulp:compile && npm run webpack:start",
    "webpack:start": "webpack-dev-server --open --output-public-path --inline --progress --port 3000",
    "webpack:build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "typings": "typings",
    "postinstall": "typings install",
    "gulp:compile": "node_modules/gulp/bin/gulp.js default",
    "gulp:development": "node_modules/gulp/bin/gulp.js dev",
    "dev": "concurrently \"npm run gulp:development\" \"npm run webpack:start\" ",
    "test": "node_modules/gulp/bin/gulp.js default && npm run webpack:build"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ultimate-comparisons/ultimate-comparison-BASE.git"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat-json": "^1.0.0",
    "gulp-exec": "^2.1.2",
    "gulp-json-transform": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-pandoc": "^0.2.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "sync-exec": "^0.6.2",
    "typescript": "~2.0.10",
    "typings": "^1.4.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
